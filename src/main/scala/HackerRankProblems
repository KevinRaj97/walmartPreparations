import scala.util.matching._
import scala.reflect._

object Result {

    /*
     * Complete the 'aVeryBigSum' function below.
     *
     * The function is expected to return a LONG_INTEGER.
     * The function accepts LONG_INTEGER_ARRAY ar as parameter.
     */

    def aVeryBigSum(ar: Array[Long]): Long = {
    // Write your code here
    var count:Long = 0
    for (i <- ar) {
      count += i
     }
    count
    }

}

object Solution {
    def main(args: Array[String]) {
        val printWriter = new PrintWriter(sys.env("OUTPUT_PATH"))

        val arCount = StdIn.readLine.trim.toInt

        val ar = StdIn.readLine.replaceAll("\\s+$", "").split(" ").map(_.trim.toLong)

        val result = Result.aVeryBigSum(ar)

        printWriter.println(result)

        printWriter.close()
    }
}

6.Maximum points:

import java.io._
import java.math._
import java.security._
import java.text._
import java.util._
import java.util.concurrent._
import java.util.function._
import java.util.regex._
import java.util.stream._
import scala.collection.immutable._
import scala.collection.mutable._
import scala.collection.concurrent._
import scala.concurrent._
import scala.io._
import scala.math._
import scala.sys._
import scala.util.matching._
import scala.reflect._

object Result {

    /*
     * Complete the 'compareTriplets' function below.
     *
     * The function is expected to return an INTEGER_ARRAY.
     * The function accepts following parameters:
     *  1. INTEGER_ARRAY a
     *  2. INTEGER_ARRAY b
     */

    def compareTriplets(a: Array[Int], b: Array[Int]): Array[Int] = {
    // Write your code here

     var point1= 0
    var point2= 0

    if(a(0)>b(0))
    { point1 =point1+ 1
    }
    else if(b(0)>a(0))
    {
    point2 =point2+ 1

    }

    if(a(1)>b(1))
    {
    point1= point1+ 1
    }
    else if(b(1)>a(1))
    {
    point2 =point2 + 1
    }


    if(a(2)>b(2))
    {
    point1= point1+ 1
    }
    else if(a(2)<b(2))
    {
    point2=point2+ 1
    }

    Array(point1.toInt,point2.toInt)
}

}

object Solution {
    def main(args: Array[String]) {
        val printWriter = new PrintWriter(sys.env("OUTPUT_PATH"))

        val a = StdIn.readLine.replaceAll("\\s+$", "").split(" ").map(_.trim.toInt)

        val b = StdIn.readLine.replaceAll("\\s+$", "").split(" ").map(_.trim.toInt)

        val result = Result.compareTriplets(a, b)

        printWriter.println(result.mkString(" "))

        printWriter.close()
    }
}


7.Plus Minus::

import java.io._
import java.math._
import java.security._
import java.text._
import java.util._
import java.util.concurrent._
import java.util.function._
import java.util.regex._
import java.util.stream._
import scala.collection.immutable._
import scala.collection.mutable._
import scala.collection.concurrent._
import scala.concurrent._
import scala.io._
import scala.math._
import scala.sys._
import scala.util.matching._
import scala.reflect._

object Result {

    /*
     * Complete the 'plusMinus' function below.
     *
     * The function accepts INTEGER_ARRAY arr as parameter.
     */

    def plusMinus(arr: Array[Int]) {
    // Write your code here
var pos: Double = 0
  var neg: Double = 0
  var zero: Double = 0

  for (o <- arr) {
    if (o > 0) {
      pos = pos + 1
    }
    else if (o < 0) {
      neg = neg + 1
    }
    else {
      zero = zero + 1
    }
  }

   //println(pos)
   //println(neg)
   //println(zero)


  //  println("%.6f \n", pos)
  //  println("%.6f \n", neg)
  //  println("%.6f \n", zero)

  // System.out.format("%.6f", pos.asInstanceOf[Double] / arr.length)
  // println()
  // System.out.format("%.6f", neg.asInstanceOf[Double] / arr.length)
  // println()
  // System.out.format("%.6f", zero.asInstanceOf[Double] / arr.length)


  printf("%f", pos.asInstanceOf[Double] / a.length)
  println()
  printf("%f", neg.asInstanceOf[Double] / a.length)
  println()
  printf("%f", zero.asInstanceOf[Double] / a.length)
}
}

object Solution {
    def main(args: Array[String]) {
        val n = StdIn.readLine.trim.toInt

        val arr = StdIn.readLine.replaceAll("\\s+$", "").split(" ").map(_.trim.toInt)

        Result.plusMinus(arr)
    }
}


